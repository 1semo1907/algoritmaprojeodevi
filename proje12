public class Proje12 {
    public static void main(String[] args) {
        String kelime = "Program";
        int uzunluk = kelime.length();

        for (int i = 0; i < uzunluk; i++) {
            
            kelime = kelime.substring(1) + kelime.charAt(0);
            System.out.println(kelime);
        }
    }
}
//proje 12
